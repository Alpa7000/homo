<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.huawei.android.launcher">
    
	
	<!--去掉google搜索栏-->
    <appwidget
        launcher:packageName="com.google.android.googlequicksearchbox"
        launcher:className="com.google.android.googlequicksearchbox.SearchWidgetProvider"
        launcher:op="delete" />
	<!--加入chrome搜索栏-->
	<appwidget
        launcher:packageName="com.android.chrome"
        launcher:className="org.chromium.chrome.browser.searchwidget.SearchWidgetProvider"
        launcher:container="-100"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="3"
        launcher:spanX="4"
        launcher:spanY="1"
		launcher:op="insertinto"/>
	<!--左起第 1 屏-->
	<!--插入HiCare-->
    <favorite
        launcher:packageName="com.huawei.phoneservice"
        launcher:className="com.huawei.phoneservice.LaunchActivity"
        launcher:container="-100"
        launcher:screen="1"
        launcher:x="0"
        launcher:y="0"
        launcher:spanX="1"
        launcher:spanY="1" 
		launcher:op="insertinto"/>
    <!--插入AppGallery-->
	<favorite
        launcher:packageName="com.huawei.appmarket"
        launcher:className="com.huawei.appmarket.MainActivity"
        launcher:container="-100"
        launcher:screen="1"
        launcher:x="1"
        launcher:y="0"
        launcher:spanX="1"
        launcher:spanY="1" 
		launcher:op="insertinto"/>
	<!--插入browser-->
    <favorite
        launcher:packageName="com.huawei.browser"
        launcher:className="com.huawei.browser.Main"
        launcher:container="-100"
        launcher:screen="1"
        launcher:x="0"
        launcher:y="1"
        launcher:spanX="1"
        launcher:spanY="1" 
		launcher:op="insertinto"/>
		
	<!--插入快捷方式-->
	<favorite
        launcher:packageName="com.huawei.android.launcher"
        launcher:className="com.huawei.launcher.cust.LauncherShortcutActivity"
        launcher:shortcutid="HuaweiDevice"
        launcher:container="-100"
        launcher:screen="1"
        launcher:x="2"
        launcher:y="2"
        launcher:spanX="1"
        launcher:spanY="1"
        launcher:shortcut="true"
		launcher:op="insertinto"/>
	
</favorites>
