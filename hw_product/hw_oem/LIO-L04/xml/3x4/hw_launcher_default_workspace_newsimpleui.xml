<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.huawei.android.launcher">
    <!-- 邮件 -->
    <favorite
        launcher:packageName="com.huawei.email"
        launcher:className="com.android.email.activity.Welcome"
        launcher:container="-100"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="2"
        launcher:spanX="1"
        launcher:spanY="1"
        launcher:op="insertinto"/>
    <!-- 日历 -->
    <favorite
